{
	"manifest_version": 2,
	"name": "Breakpoint Tester",
	"version": "0.0.0.1",
	"description": "Easily switch between predefined (or user-defined) breakpoints to test a websites responsiveness.",
	"icons": {
		"16": "images/logo16.png",
		"48": "images/logo48.png",
		"128": "images/logo128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "images/logo19.png",
			"38": "images/logo38.png"
		},
		"default_title": "Breakpoint Tester",
		"default_popup": "popup.html"
	},
	"author": "Jeffrey Hyer",
	"homepage_url": "http://github.com/JeffreyHyer/Responsive-Breakpoint-Tester/",
	"offline_enabled": true
}