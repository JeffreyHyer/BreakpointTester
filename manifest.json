{
	"manifest_version": 2,
	"name": "Breakpoint Tester",
	"version": "1.0.1",
	"description": "Easily switch between predefined (or user defined) breakpoints to test responsiveness.",
	"icons": {
		"16": "img/logo16.png",
		"48": "img/logo48.png",
		"128": "img/logo128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "img/logo19.png",
			"38": "img/logo38.png"
		},
		"default_title": "Breakpoint Tester",
		"default_popup": "popup.html"
	},
	"permissions": [
  		"storage",
  		"https://www.google-analytics.com/"
  	],
  	"content_security_policy": "script-src 'self' https://www.google-analytics.com/; object-src 'self'",
	"author": "Jeffrey Hyer",
	"homepage_url": "https://github.com/JeffreyHyer/BreakpointTester",
	"offline_enabled": true
}